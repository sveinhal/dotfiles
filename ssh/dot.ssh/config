CanonicalizeHostname yes
CanonicalDomains stork-wall.ts.net
CanonicalizeMaxDots 0

Host *
  IgnoreUnknown UseKeychain
  AddKeysToAgent yes
  UseKeychain yes
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_ed25519

Host *.stork-wall.ts.net
  ForwardAgent yes

Host *.samfundet.no login.samfundet.no
  ForwardAgent yes
  RemoteForward 0 localhost:22

# Connection back to origin machine via reverse tunnel
Host back
  HostName localhost
  User sveinhal
  Port 22
  ProxyCommand ~/.dotfiles/ssh/origin-proxy %h %p
